{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "crons": [
    {
      "path": "/api/event-extract",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/opr-extract",
      "schedule": "0 2 * * *"
    }
  ]
}