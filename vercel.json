{
  "builds": [
    {
      "src": "ML/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/mlapi/:path*",
      "destination": "/ML/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/event-extract",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/opr-extract",
      "schedule": "0 2 * * *"
    }
  ]
}