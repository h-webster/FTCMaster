{
  "builds": [
    {
      "src": "ML/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/mlapi/(.*)",
      "dest": "ML/index.py"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/event-extract",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/opr-extract",
      "schedule": "0 2 * * *"
    }
  ]
}
